@model Microsoft.AspNet.Diagnostics.Views.ErrorPageModel
<html>
<head>
    <link rel="stylesheet" href="/content/css/main.min.css">
    <link href="/content/js/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <style>
        body {
            background-image: url(data:image/png;base64,);
        }

        .error-template {
            padding: 40px 15px;
            text-align: center;
        }

        .error-actions {
            margin-top: 15px;
            margin-bottom: 15px;
        }

            .error-actions .btn {
                margin-right: 10px;
            }
    </style>
</head>
<body>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="error-template">
                <h1>
                    Oops!
                </h1>
                <h2>
                    500 Internal Server Error
                </h2>
                <div class="error-details">
                    Sorry, an error has occured, Requested page not found!
                    @if (Model != null)
                    {
                        <div>

                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Exception</a></li>
                                <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Headers</a></li>
                                <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">QueryStrings</a></li>
                                <li role="presentation"><a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">Cookies</a></li>
                            </ul>

                            <!-- Tab panes -->
                            <div class="tab-content">
                                <div role="tabpanel" class="tab-pane active align-left" id="home">
                                    <ul>
                                        @foreach (var error in Model.ErrorDetails)
                                        {
                                        <li>@error.Error.Message</li>
                                        }
                                    </ul>
                                </div>
                                <div role="tabpanel" class="tab-pane" id="profile">...</div>
                                <div role="tabpanel" class="tab-pane" id="messages">...</div>
                                <div role="tabpanel" class="tab-pane" id="settings">...</div>
                            </div>

                        </div>
                    }

                </div>
                <div class="error-actions">
                    <a href="//@Html.Panther().Site.Url" class="btn btn-primary btn-lg">
                        <span class="glyphicon glyphicon-home"></span>
                        Take Me Home
                    </a>
                    <a href="http://www.jquery2dotnet.com" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-envelope"></span> Contact Support </a>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $('#myTabs a').click(function (e) {
        e.preventDefault();
        $(this).tab('show');
    })
</script>
    <script src="/content/js/bootstrap/js/bootstrap.min.js"></script>
    <script src="/content/js/jquery/js/jquery.min.js"></script>
</body>
</html>
